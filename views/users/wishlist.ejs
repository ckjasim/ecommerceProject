    <script>
        function addToCart(productId) {
            const quantity = document.getElementById('quantityInput').value;
                  
            console.log(productId)
            axios.post('/loadCart', { quantity,productId })
                .then(function(response) {
                    console.log(response.data);
                    const {session} =response.data
    
                    if(response.data){
                        if (session==false) {
                            console.log('1111');
                            window.location='/notLogin'
                            
                        } else {
                            
                            window.location='/viewCart'
                        }  
                    }
                    
                })
                .catch(function(error) {
                    console.error('Error:', error);
                });
        };

        async function deleteWishlistProduct(productId) {
  
        axios.post('/deleteWishlistProduct', { productId })
        
            .then(function(response) {  
            const element = document.getElementById(`wishlistRefresh${productId}`)
            element.remove();
            window.location.reload();
                        
            console.log(response.data);
            })
            .catch(function(error) {
            console.error('Error:', error);
            })
            }
    </script>

