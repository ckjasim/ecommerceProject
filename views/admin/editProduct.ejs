        <script>
     function checkValidity() {
        var nameError = document.getElementById('nameError').textContent;
        var quantityError = document.getElementById('quantityError').textContent;
        var colorError = document.getElementById('colorError').textContent;
        var sizeError = document.getElementById('sizeError').textContent;
        var materialError = document.getElementById('materialError').textContent;
        var priceError = document.getElementById('priceError').textContent;
        var descriptionError = document.getElementById('descriptionError').textContent;
    
        var submitButton = document.getElementById('submitButton');

        if (
            nameError === "" &&
            quantityError === "" &&
            colorError === "" &&
            sizeError === "" &&
            materialError === "" &&
            priceError === "" &&
            descriptionError === ""
        
        ) {
            submitButton.disabled = false;
        } else {
            submitButton.disabled = true;
        }
    }
            


            function validateName() {
                var productName = document.getElementById('productName').value.trim();
                var nameError = document.getElementById('nameError');
        
                var namePattern = /^[a-zA-Z][a-zA-Z\s]*$/;
        
                if (!productName || !namePattern.test(productName)) {
                    nameError.textContent = 'invalid name provided';
                } else {
                    nameError.textContent = '';
                }
            }
           
            function validateQuantity() {
                var productQuantity = document.getElementById('productQuantity').value.trim();
                var quantityError = document.getElementById('quantityError');

                var quantityPattern = /^[1-9]\d*$/;

                if (!productQuantity || !quantityPattern.test(productQuantity)) {
                    quantityError.textContent = 'Enter a valid number';
                } else {
                    quantityError.textContent = '';
                }
            }


            function validateColor() {
                var productColor = document.getElementById('productColor').value.trim();
                var colorError = document.getElementById('colorError');
        
                var namePattern = /^[a-zA-Z][a-zA-Z\s]*$/;
        
                if (!productColor || !namePattern.test(productColor)) {
                    colorError.textContent = 'invalid color provided';
                } else {
                    colorError.textContent = '';
                }
            }
            function validateSize() {
                var productQuantity = document.getElementById('productSize').value.trim();
                var quantityError = document.getElementById('sizeError');

                var quantityPattern = /^[a-zA-Z][a-zA-Z\s]*$/;

                if (!productQuantity || !quantityPattern.test(productQuantity)) {
                    quantityError.textContent = 'Enter a valid size';
                } else {
                    quantityError.textContent = '';
                }
            }
            function validateMaterial() {
                var productName = document.getElementById('productMaterial').value.trim();
                var nameError = document.getElementById('materialError');
        
                var namePattern = /^[a-zA-Z][a-zA-Z\s]*$/;
        
                if (!productName || !namePattern.test(productName)) {
                    nameError.textContent = 'Invalid material provided';
                } else {
                    nameError.textContent = '';
                }
            }
          

           function validatePrice() {
                var productQuantity = document.getElementById('productPrice').value.trim();
                var quantityError = document.getElementById('priceError');

                var quantityPattern = /^[1-9]\d*$/;

                if (!productQuantity || !quantityPattern.test(productQuantity)) {
                    quantityError.textContent = 'Enter a valid number';
                } else {
                    quantityError.textContent = '';
                }
            }

            function validateDescription() {
                var productName = document.getElementById('productDescription').value.trim();
                var nameError = document.getElementById('descriptionError');
        
                var namePattern = /^[a-zA-Z][a-zA-Z\s]*$/;
        
                if (!productName || !namePattern.test(productName)) {
                    nameError.textContent = 'Invalid material provided';
                } else {
                    nameError.textContent = '';
                }
            }

            document.getElementById('productName').addEventListener('input', function() {
                validateName();
                checkValidity(); 
        });
            document.getElementById('productQuantity').addEventListener('input', function() {
                validateQuantity();
                checkValidity(); 
        });
    
            document.getElementById('productColor').addEventListener('input', function() {
                validateColor();
                checkValidity(); 
        });
    
            document.getElementById('productSize').addEventListener('input', function() {
                validateSize();
                checkValidity(); 
        });
    
            document.getElementById('productMaterial').addEventListener('input', function() {
                validateMaterial();
                checkValidity(); 
        });
            document.getElementById('productPrice').addEventListener('input', function() {
                validatePrice();
                checkValidity(); 
        });
            document.getElementById('productDescription').addEventListener('input', function() {
                validateDescription();
                checkValidity(); 
        });
           

        
    
        </script>
        
        
